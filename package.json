{
  "name": "commitlint-plugin-empty-issues-rule",
  "version": "0.0.0",
  "description": "commitlint rule plugin to check include issues",
  "author": "Memory Lovers, LLC<https://github.com/memorylovers>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/memory-lovers",
  "homepage": "https://github.com/memorylovers/commitlint-plugin-empty-issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/memorylovers/commitlint-plugin-empty-issues.git"
  },
  "bugs": {
    "url": "https://github.com/memorylovers/commitlint-plugin-empty-issues/issues"
  },
  "keywords": [
    "commitlint",
    "commitlintplugin",
    "plugin",
    "rules"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "16"
  },
  "packageManager": "pnpm@7.17.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:ts && pnpm build:dts",
    "build:ts": "tsc --noEmit && vite build",
    "build:dts": "tsc --emitDeclarationOnly",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "changelog": "pnpx changelogen"
  },
  "devDependencies": {
    "@commitlint/types": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.8.0",
    "prettier-plugin-md-nocjsp": "^1.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.3"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "ignorePatterns": [
      "**/*.test.ts",
      "dist"
    ]
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "*.md",
          "README"
        ],
        "options": {
          "parser": "markdown-nocjsp"
        }
      }
    ]
  }
}
